{
  "ReverseProxy": {
    "Routes": {
      "legacy_api_v1": {
        "ClusterId": "legacy_node_cluster",
        "Match": { "Path": "/api/v1/{**catch-all}" },
        "Transforms": [{ "PathPattern": "/api/{catch-all}" }]
      },
      "legacy_incidents": {
        "ClusterId": "legacy_node_cluster",
        "Match": { "Path": "/api/incidents/{**catch-all}" }
      },
      "legacy_documents": {
        "ClusterId": "legacy_node_cluster",
        "Match": { "Path": "/api/documents/{**catch-all}" }
      },
      "legacy_processes": {
        "ClusterId": "legacy_node_cluster",
        "Match": { "Path": "/api/processes/{**catch-all}" }
      },
      "legacy_dependencies": {
        "ClusterId": "legacy_node_cluster",
        "Match": { "Path": "/api/dependencies/{**catch-all}" }
      },
      "legacy_resources": {
        "ClusterId": "legacy_node_cluster",
        "Match": { "Path": "/api/resources/{**catch-all}" }
      },
      "legacy_resource_deps": {
        "ClusterId": "legacy_node_cluster",
        "Match": { "Path": "/api/resource-dependencies/{**catch-all}" }
      },
      "legacy_links": {
        "ClusterId": "legacy_node_cluster",
        "Match": { "Path": "/api/process-resource-links/{**catch-all}" }
      },
      "legacy_risk_treatments": {
        "ClusterId": "legacy_node_cluster",
        "Match": { "Path": "/api/risk-treatments/{**catch-all}" }
      }
    },
    "Clusters": {
      "legacy_node_cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:3001"
          }
        }
      }
    }
  }
}
